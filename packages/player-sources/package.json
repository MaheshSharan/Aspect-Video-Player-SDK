{
    "name": "aspect-player-sources",
    "version": "0.1.3",
    "description": "HLS, DASH, and MP4 source adapters for Aspect Video Player",
    "license": "MIT",
    "author": "Mahesh Sharan",
    "repository": {
        "type": "git",
        "url": "https://github.com/MaheshSharan/Aspect-Video-Player-SDK.git",
        "directory": "packages/player-sources"
    },
    "keywords": [
        "hls",
        "dash",
        "mp4",
        "video-streaming",
        "adaptive-streaming"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "aspect-player-shared": "^0.1.3",
        "aspect-player-core": "^0.1.3",
        "hls.js": "^1.5.0",
        "dashjs": "^4.7.0"
    },
    "devDependencies": {
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    }
}