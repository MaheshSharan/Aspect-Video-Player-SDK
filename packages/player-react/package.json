{
    "name": "aspect-player-react",
    "version": "0.1.8",
    "description": "React wrapper for Aspect Video Player SDK",
    "license": "MIT",
    "author": "Mahesh Sharan",
    "repository": {
        "type": "git",
        "url": "https://github.com/MaheshSharan/Aspect-Video-Player-SDK.git",
        "directory": "packages/player-react"
    },
    "keywords": [
        "video-player",
        "react",
        "hls",
        "dash",
        "streaming",
        "media-player"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "aspect-player-shared": "^0.1.5",
        "aspect-player-core": "^0.1.5",
        "aspect-player-sources": "^0.1.5",
        "aspect-player-ui": "^0.1.5"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    }
}
